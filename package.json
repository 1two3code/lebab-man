{
  "name": "pacman-html5",
  "description": "Pacman written in TypeScript",
  "version": "3.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobbylight/PacmanJS.git"
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack/webpack.config.js",
    "watch": "cross-env NODE_ENV=dev webpack-dev-server --config ./webpack/webpack.config.js",
    "start": "electron src/js/electron-main.js",
    "lint": "tslint --project .",
    "test": "cross-env NODE_ENV=test mocha --recursive --compilers ts:ts-node/register \"src/app/**/*.spec.ts\"",
    "cover": "cross-env NODE_ENV=test nyc npm test"
  },
  "nyc": {
    "include": [
      "src/app/**/*.ts"
    ],
    "exclude": [
      "src/app/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/mocha": "5.2.4",
    "chai": "4.1.2",
    "copy-webpack-plugin": "4.5.2",
    "cross-env": "5.2.0",
    "css-loader": "0.28.11",
    "electron": "2.0.3",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.4.1",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "style-loader": "0.21.0",
    "ts-loader": "4.4.2",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "tslint-loader": "3.6.0",
    "typescript": "2.9.2",
    "webpack": "4.14.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "dependencies": {
    "gtp": "2.0.0"
  }
}

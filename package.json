{
  "name": "pacman-html5",
  "description": "Pacman written in TypeScript",
  "version": "3.0.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobbylight/PacmanJS.git"
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack/webpack.config.js",
    "watch": "cross-env NODE_ENV=dev webpack-dev-server --config ./webpack/webpack.config.js",
    "start": "electron src/js/electron-main.js",
    "lint": "tslint --project .",
    "test": "cross-env NODE_ENV=test mocha --recursive --require ts-node/register \"src/app/**/*.spec.ts\"",
    "cover": "cross-env NODE_ENV=test nyc npm test"
  },
  "nyc": {
    "include": [
      "src/app/**/*.ts"
    ],
    "exclude": [
      "src/app/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.7",
    "chai": "4.2.0",
    "copy-webpack-plugin": "5.0.3",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "electron": "5.0.2",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.7.0",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "raw-loader": "2.0.0",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "ts-loader": "6.0.2",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.5.1",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.5.1",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "dependencies": {
    "gtp": "2.0.4"
  }
}
